# This is a file to use with [Rancher Rio](https://rio.io)

services:
  rancher-demo:
    app: rancher-demo
    version: v1
    ports: 80:8080/http
    env:
    - COW_COLOR=yellow
    autoscale:
      concurrency: 10
      maxReplicas: 5
      minReplicas: 1
    build:
      branch: master
      pr: true
      repo: https://github.com/oskapt/rancher-demo
      webhookSecretName: githubtoken
    template: true

